            <Image url="@../images/1675839423933.jpg" />
         </image>
      </ImageView>
      <Label fx:id="usernamelable" layoutX="119.0" layoutY="271.0" prefHeight="30.0" prefWidth="94.0" text="User name :">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="passwordlable" layoutX="119.0" layoutY="353.0" prefHeight="26.0" prefWidth="88.0" text="PassWord :">

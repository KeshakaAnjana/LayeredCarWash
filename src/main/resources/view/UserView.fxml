<AnchorPane fx:id="loginLayout" minHeight="750.0" minWidth="1150.0" prefHeight="546.0" prefWidth="794.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ijse.gdse.carwash.controller.UserController">
   <children>
      <ImageView fitHeight="750.0" fitWidth="1150.0" opacity="0.53">
